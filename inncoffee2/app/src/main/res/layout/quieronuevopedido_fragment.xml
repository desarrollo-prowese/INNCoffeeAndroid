<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/whatsapp_image_2019_07_23_at_15_12_59"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/EntrarMapa"
            android:layout_width="0dp"
            android:layout_height="293dp"
            android:layout_marginEnd="59dp"
            android:layout_marginStart="59dp"
            android:layout_marginTop="130dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/trazado_52_ek13" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="129dp"
            android:layout_height="0dp"
            android:layout_marginBottom="11dp"
            android:layout_marginTop="170dp"
            app:layout_constraintBottom_toTopOf="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/trazado_114" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="220dp"
            android:layout_height="0dp"
            android:layout_marginBottom="207dp"
            android:text="@string/selecciona_establecimiento_string"
            android:textAlignment="center"
            android:textColor="#FDFDFE"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView10" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>