<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/nuevopedido2">

    <Button
        android:id="@+id/cartadesayuno"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:background="@drawable/rect_ngulo_1471_ek3_shape"
        android:text="@string/carta_desayunos_ek1_string"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.624" />

    <Button
        android:id="@+id/cartacomidas"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:background="@drawable/rect_ngulo_1484_shape"
        android:text="@string/carta_comidas_ek10_string"
        android:textColor="#000000"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cartadesayuno"
        app:layout_constraintVertical_bias="0.086" />

    <Button
        android:id="@+id/cartamerienda"
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:background="@drawable/rect_ngulo_1484_shape"
        android:text="Carta Merienda"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/alojenos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cartacomidas"
        app:layout_constraintVertical_bias="0.39" />

    <Button
        android:id="@+id/alojenos"
        android:layout_width="270dp"
        android:layout_height="60dp"
        android:background="@drawable/rect_ngulo_1471_ek1_shape"
        android:text="@string/tienes_alg_n_tipo_de_alergia_alimentaria"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#FFFFFF"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cartacomidas"
        app:layout_constraintVertical_bias="0.71000004" />
</androidx.constraintlayout.widget.ConstraintLayout>