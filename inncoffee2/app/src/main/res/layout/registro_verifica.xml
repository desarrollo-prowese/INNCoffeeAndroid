<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayoutQuiero"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/whatsapp_image_2019_07_23_at_15_12_24_2_"
    android:scrollbarSize="0dp">


    <TextView
        android:layout_width="253dp"
        android:layout_height="120dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/menu_registronew1"
        android:padding="5dp"
        android:text="@string/para_terminar_tu_registro_verifica_tu_correo_electr_nico_mediante_el_enlace_que_acabamos_de_enviarte_por_mail__string"
        android:textAlignment="center"
        android:textColor="@android:color/background_light"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/buttonMisPedidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.653" />

    <Button
        android:id="@+id/rect_ngulo_1461_ek12"
        android:layout_width="253dp"
        android:layout_height="50dp"
        android:background="@drawable/registro_negro"
        android:text="@string/iniciar_sesi_n_ek2_string"
        android:textColor="@android:color/background_light"
        android:textSize="23sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.717" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignBottom="@+id/buttonNuevoPedido"
        android:layout_alignEnd="@+id/buttonNuevoPedido"
        android:layout_alignStart="@+id/buttonNuevoPedido"
        android:layout_alignTop="@+id/buttonNuevoPedido"
        android:background="@drawable/inncoffeelogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.338" />

</androidx.constraintlayout.widget.ConstraintLayout>